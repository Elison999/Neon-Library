local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/Elison999/Neon-Library/refs/heads/main/Neon%20Lira"))()

    local Window = Library:CreateWindow({
        Name = "Titan Script Hub",
        KeySystem = true, -- Ative para testar
        KeyName = "KeySystem Key",
        Key = "1234",
        GrabKeyFromSite = false, -- Se true, ele pega a Key do site abaixo
        KeySite = "https://pastebin.com/raw/..."
    })

    local Tab1 = Window:Tab("Main")
    local Tab2 = Window:Tab("Player")

    Tab1:Label("Welcome User!")

    Tab1:Button("Kill All", function()
        print("Killed everyone")
        Library:Notification({
            Title = "Action",
            Content = "Killed all players!",
            Duration = 3
        })
    end)

    Tab1:Toggle("Auto Farm", false, function(state)
        print("Auto Farm is now:", state)
    end)

    Tab1:Slider("WalkSpeed", 16, 100, 16, function(value)
        game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = value
    end)

    Tab1:Dropdown("Select Weapon", {"Sword", "Bow", "Gun"}, function(selected)
        print("Selected:", selected)
    end)

    Tab1:Textbox("Webhook", "Paste URL here", function(text)
        print("Webhook set to:", text)
    end)

    Tab2:Keybind("Fly Key", Enum.KeyCode.F, function()
        print("Flight toggled")
    end)
